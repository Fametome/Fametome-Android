    <string name="demo_message_flash_1">Bienvenu sur Fametome !</string>
    <string name="demo_message_flash_2">Ceci est un flash. Il peut être composé de sms, de photos et de faces !</string>
    <string name="demo_message_flash_3">Vous allez voir un sms, une photo et une face.</string>
